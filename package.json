  "keywords": [
    "vue",
    "class",
    "migrate"
  ],
  "devDependencies": {
    "@types/eslint": "8.44.0",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.0.0",
    "commander": "^11.0.0",
    "eslint": "8.45.0",
    "jest": "^29.6.2",
    "ts-jest": "^29.1.1",
    "ts-morph": "^19.0.0",
    "ts-node": "^10.9.1",
